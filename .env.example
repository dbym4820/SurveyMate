APP_NAME=AutoSurvey
APP_ENV=local
APP_KEY=
APP_DEBUG=true
# APP_URL and ASSET_URL are auto-detected from request - leave empty for portability
APP_URL=
ASSET_URL=
APP_TIMEZONE=Asia/Tokyo

APP_LOCALE=ja
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=ja_JP

APP_MAINTENANCE_DRIVER=file

BCRYPT_ROUNDS=10

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# MySQL Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=autosurvey
DB_USERNAME=root
DB_PASSWORD=

# Session (use 'file' for custom session management)
SESSION_DRIVER=file
SESSION_LIFETIME=1440
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database

# AI Providers
AI_PROVIDER=claude
CLAUDE_API_KEY=
CLAUDE_MODEL=claude-sonnet-4-5-20250929
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o

# RSS Fetching
FETCH_SCHEDULE="0 6 * * *"
FETCH_ENABLED=true
FETCH_MIN_INTERVAL=5000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:5173

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

VITE_APP_NAME="${APP_NAME}"
